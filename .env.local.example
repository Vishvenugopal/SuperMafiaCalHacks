# LiveKit for realtime voice (get from https://cloud.livekit.io)
LIVEKIT_WS_URL=wss://your-project.livekit.cloud
LIVEKIT_API_KEY=your_api_key
LIVEKIT_API_SECRET=your_api_secret

# JanitorAI for host personality (get from https://janitorai.com)
JANITOR_AI_API_KEY=your_janitor_api_key
JANITOR_AI_CHARACTER_ID=your_character_id

# Voice settings
ENABLE_WAKEWORD=false
STT_PROVIDER=livekit
TTS_PROVIDER=livekit

# App settings
NEXT_PUBLIC_APP_NAME=SuperMafia
