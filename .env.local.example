# LiveKit for realtime voice (get from https://cloud.livekit.io)
# Leave empty to use Web Speech API fallback
LIVEKIT_WS_URL=
LIVEKIT_API_KEY=
LIVEKIT_API_SECRET=

# Baseten for AI narrator/host (get from https://baseten.co)
# Leave empty to use mock host fallback
BASETEN_API_KEY=
BASETEN_MODEL_ID=

# JanitorAI for host personality (get from https://janitorai.com)
# Leave empty to use mock host fallback
JANITOR_AI_API_KEY=
JANITOR_AI_CHARACTER_ID=

# ElevenLabs for high-quality TTS (get from https://elevenlabs.io)
# Leave empty to use Web Speech API fallback
ELEVENLABS_API_KEY=
ELEVENLABS_VOICE_ID=

# Voice settings (ignored if LiveKit not configured)
ENABLE_WAKEWORD=false

# App settings
NEXT_PUBLIC_APP_NAME=SuperMafia
